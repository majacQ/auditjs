{
  "name": "auditjs",
  "version": "4.0.25",
  "description": "Audit dependencies to identify known vulnerabilities and maintenance problems",
  "main": "./bin/index.js",
  "bin": {
    "auditjs": "./bin/index.js"
  },
  "files": [
    "bin/**/*"
  ],
  "preferGlobal": true,
  "engines": {
    "node": ">=8.0.0"
  },
  "resolutions": {
    "hosted-git-info": "^3.0.8"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "build": "tsc -p tsconfig.json",
    "build-dev": "tsc -p tsconfig.development.json",
    "coverage": "nyc mocha -r ts-node/register src/**/*.spec.ts",
    "generate-coverage-report": "nyc report --reporter=text-lcov > lcov.info",
    "test-ci": "MOCHA_FILE=./reports/test-results.xml mocha -r ts-node/register src/**/*.spec.ts --reporter mocha-junit-reporter",
    "lint": "eslint src/**/*.ts",
    "start": "node ./bin/index.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test",
    "iq-scan": "npx auditjs@latest iq -a auditjs"
  },
  "keywords": [
    "Audit",
    "Scan",
    "Vulnerability",
    "CPE",
    "CVE",
    "CWE",
    "OSSIndex",
    "Artie",
    "Nexus",
    "Sonatype",
    "SCA",
    "SBOM",
    "Security",
    "Advisories"
  ],
  "author": "Sonatype Community",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sonatype-nexus-community/auditjs.git"
  },
  "devDependencies": {
    "@types/chai": "4.2.0",
    "@types/chai-as-promised": "7.1.1",
    "@types/figlet": "^1.5.4",
    "@types/js-yaml": "^3.12.7",
    "@types/mocha": "5.2.7",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^12.20.42",
    "@types/node-fetch": "^2.5.12",
    "@types/node-persist": "^3.1.2",
    "@types/rimraf": "^2.0.5",
    "@types/sinon": "^7.5.2",
    "@types/ssri": "^6.0.3",
    "@types/uuid": "^3.4.10",
    "@types/yargs": "^13.0.12",
    "@types/yarnpkg__lockfile": "^1.1.5",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@yarnpkg/lockfile": "^1.1.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "mocha": "^8.4.0",
    "mocha-junit-reporter": "^1.23.3",
    "mock-fs": "^4.14.0",
    "nock": "11.7.0",
    "nyc": "^15.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "sinon": "^8.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "colors": "^1.3.1",
    "figlet": "^1.5.2",
    "https-proxy-agent": "^5.0.0",
    "js-yaml": "3.13.1",
    "log4js": "^6.4.0",
    "node-fetch": "^3.2.0",
    "node-persist": "^3.1.0",
    "ora": "^4.1.1",
    "read-installed": "~4.0.3",
    "spdx-license-ids": "^3.0.11",
    "ssri": "^8.0.1",
    "uuid": "^3.4.0",
    "xmlbuilder": "^13.0.2",
    "xmldom": "^0.6.0",
    "yargs": "^16.2.0"
  }
}
